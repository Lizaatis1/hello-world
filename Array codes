package arrays;

/**
 *
 * @author Liz
 */
public class Arrays {

   
    public static void main(String[] args) {
        //finding a missing number
        int[] k = {3,5,4,1};
        int n = k.length + 1;
        int sum = (n * (n+1))/2;
        for(int i = 0; i < k.length; i++){
            sum = sum - k[i];
        }
        System.out.println(" Missing number is " + sum);
        }
    }
    

